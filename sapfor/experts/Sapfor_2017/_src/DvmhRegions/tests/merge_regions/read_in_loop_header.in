		program jac2d
		parameter (l = 8000,itmax = 100)
		real  a(l,l),eps,maxeps,b(l,l)
		double precision  startt,endt,dvtime


        b(1,1) = 2.0
        do  j = 1,l+eps
            do  i = 1,l
            a(i,j) = i*j
            enddo
        enddo
        b(1,1) = 3.0
        a(1,1) = 10
        eps = 15
        do  j = 1,l
            do  i = 1,l
            b(i,j) = 0. + a(i,j) + eps
            enddo
        enddo

        maxeps = eps
        end